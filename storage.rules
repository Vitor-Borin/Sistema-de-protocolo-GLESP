rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Regras para upload de arquivos
    match /protocolos/{allPaths=**} {
      // Apenas usu치rios autenticados podem fazer upload
      allow read, write: if request.auth != null;
    }
    
    // Regras para avatares de usu치rios
    match /avatars/{allPaths=**} {
      // Usu치rios podem fazer upload de seus pr칩prios avatares
      allow read, write: if request.auth != null;
    }
  }
}
