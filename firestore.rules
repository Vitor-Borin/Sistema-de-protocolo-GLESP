rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // REGRAS TEMPORÁRIAS PERMISSIVAS PARA CORRIGIR O ERRO
    // TODO: Ajustar para produção após testes
    
    // Todas as coleções - usuários autenticados podem ler/escrever
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}